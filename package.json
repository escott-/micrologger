{
  "name": "micrologger",
  "version": "2.0.5",
  "description": "koa microservice application and request logging",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/.bin/nyc ./node_modules/.bin/mocha"
  },
  "pre-commit": [
    "test"
  ],
  "nyc": {
    "check-coverage": true,
    "statements": 80,
    "functions": 65,
    "branches": 75,
    "lines": 85,
    "all": true,
    "exclude": [
      "**/*.spec.js",
      "examples",
      "coverage",
      "test"
    ],
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/escott-/micrologger"
  },
  "keywords": [
    "log",
    "logs",
    "logging",
    "koa",
    "koa logger",
    "koa logs",
    "koa-logs",
    "middleware",
    "microservice",
    "microservice logs"
  ],
  "author": "Erik Scott",
  "license": "MIT",
  "dependencies": {
    "app-root-path": "^2.0.1",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "fluent-logger": "^2.2.0",
    "humanize-number": "0.0.2",
    "logrotate-stream": "^0.2.5",
    "mocka": "0.0.1",
    "nyc": "^10.3.2",
    "strip-ansi": "^3.0.1",
    "uuid": "^3.0.1",
    "zmq": "^2.15.3"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.2.0",
    "mockery": "^2.0.0",
    "sinon": "^2.3.1",
    "pre-commit": "^1.2.2"
  }
}
